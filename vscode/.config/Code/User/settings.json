{
  "redhat.telemetry.enabled": false,
  "editor.fontFamily": "'JetBrainsMono Nerd Font', Menlo, Monaco, Consolas, 'Droid Sans Mono', 'Courier New', monospace",
  "editor.fontLigatures": true,
  "window.zoomLevel": 1.0,
  "github.experimental.multipleAccounts": true,
  "telemetry.telemetryLevel": "off",
  "python.analysis.ignore": [
    // "*"
  ],
  "remote.extensionKind": {
    "alefragnani.project-manager": [
      "workspace"
    ]
  },
  "editor.smoothScrolling": true,
  "editor.cursorSurroundingLines": 10,
  "vscode-neovim.neovimExecutablePaths.linux": "nvim",
  "vscode-neovim.useWSL": true,
  "workbench.activityBar.orientation": "vertical",
  // open json editor for settings
  "workbench.settings.editor": "json",
  // Font size
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  // ui
  "editor.minimap.enabled": true,
  "editor.minimap.autohide": "mouseover",
  "window.commandCenter": false,
  "editor.scrollbar.vertical": "auto",
  "editor.scrollbar.horizontal": "auto",
  "workbench.startupEditor": "none",
  "editor.linkedEditing": true,
  "breadcrumbs.filePath": "on",
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.experimental.asyncTokenization": true,
  "vscode-neovim.afterInitConfig": [
    "set cmdheight=2"
  ],
  "git.openRepositoryInParentFolders": "never",
  "terminal.integrated.allowChords": false,
  "cursor.general.enableShadowWorkspace": true,
  "explorer.autoRevealExclude": {
    "**/.venv": true
  },
  "search.exclude": {
    "**/.venv": true
  },
  "terminal.external.linuxExec": "/bin/zsh",
  "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
  "python.terminal.activateEnvironment": false,
  "chat.agent.enabled": true,
  "editor.rulers": [
    120
  ],
  "httpyac.cookieJarEnabled": true,
  "ruff.lineLength": 120,
  "remote.autoForwardPortsSource": "hybrid",
  "editor.cursorBlinking": "solid",
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "github.copilot.chat.edits.temporalContext.enabled": true,
  "github.copilot.chat.languageContext.typescript.enabled": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "python.analysis.aiCodeActions": {
    "implementAbstractClasses": true
  },
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.plusFeatures.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.currentLine.pullRequests.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.statusBar.pullRequests.enabled": false,
  "gitlens.hovers.avatars": false,
  "gitlens.blame.avatars": false,
  "git.suggestSmartCommit": false,
  "workbench.editor.showTabs": "single",
  "zenMode.showTabs": "single",
  "window.density.editorTabHeight": "compact",
  "workbench.commandPalette.experimental.suggestCommands": true,
  "chat.agent.maxRequests": 50,
  "debugpy.showPythonInlineValues": true,
  "gitlens.ai.model": "vscode",
  "github.copilot.chat.codeGeneration.useInstructionFiles": true,
  "github.copilot.chat.generateTests.codeLens": true,
  "gitlens.ai.modelOptions.temperature": 0.1,
  "python.REPL.sendToNativeREPL": true,
  "window.customTitleBarVisibility": "auto",
  "github.copilot.chat.codesearch.enabled": true,
  "[python]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "update.releaseTrack": "prerelease",
  "cursorpyright.analysis.ignore": [],
  "workbench.settings.showAISearchToggle": true,
  "telemetry.editStats.enabled": false,
  "github.copilot.chat.languageContext.inline.typescript.enabled": true,
  "github.copilot.chat.languageContext.fix.typescript.enabled": true,
  "python.analysis.enableTroubleshootMissingImports": true,
  "playwright.showTrace": false,
  "playwright.reuseBrowser": true,
  "python.analysis.typeCheckingMode": "basic",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "editor.aiStats.enabled": true,
  "github.copilot.chat.scopeSelection": true,
  "chat.agentSessionsViewLocation": "view",
  "chat.tools.terminal.autoApprove": {
    "rm": false,
    "rmdir": false,
    "del": false,
    "kill": false,
    "curl": false,
    "wget": false,
    "eval": false,
    "chmod": false,
    "chown": false,
    "/^Remove-Item\\b/i": false
  },
  "chat.todoListTool.enabled": true,
  "chat.math.enabled": true,
  "accessibility.openChatEditedFiles": false,
  "inlineChat.notebookAgent": true,
  "inlineChat.enableV2": true,
  "cursor.composer.shouldAllowCustomModes": true,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "chat.edits2.enabled": true,
  "gitlens.ai.vscode.model": "copilot:gpt-5",
  "gitlens.views.scm.grouped.views": {
    "commits": true,
    "branches": true,
    "remotes": true,
    "stashes": true,
    "tags": true,
    "worktrees": true,
    "contributors": true,
    "fileHistory": false,
    "repositories": true,
    "searchAndCompare": true,
    "launchpad": false
  },
  "chat.mcp.gallery.enabled": true,
  "vite.autoStart": false,
  "github.copilot.chat.useResponsesApi": true,
  "github.copilot.chat.alternateGptPrompt.enabled": true,
  "git.allowForcePush": true,
  "python.analysis.autoImportCompletions": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "claudeCode.preferredLocation": "panel",
  "claudeCode.selectedModel": "default",
  "scm.repositories.explorer": true,
  "scm.repositories.selectionMode": "single",
  "cursor.cpp.enablePartialAccepts": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.callArgumentNames": "all",
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "typescript.inlayHints.enumMemberValues.enabled": true,
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "vue.inlayHints.destructuredProps": true,
  "vue.inlayHints.inlineHandlerLeading": true,
  "vue.inlayHints.missingProps": true,
  "vue.inlayHints.optionsWrapper": true,
  "vue.inlayHints.vBindShorthand": true,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "chat.useAgentSkills": true,
  "python.languageServer": "None",
  "git.blame.ignoreWhitespace": true,
  "gitlens.blame.ignoreWhitespace": true,
  "debug.breakpointsView.presentation": "tree",
  "chat.viewSessions.orientation": "stacked"
}
